{
  "name": "frontend-common",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "development": "./index.ts",
      "default": "./dist/index.js"
    },
    "./auth": {
      "types": "./auth/index.ts",
      "development": "./auth/index.ts",
      "default": "./dist/auth/index.js"
    },
    "./api": {
      "types": "./api/index.ts",
      "development": "./api/index.ts",
      "default": "./dist/api/index.js"
    },
    "./chat": {
      "types": "./chat/index.ts",
      "development": "./chat/index.ts",
      "default": "./dist/chat/index.js"
    },
    "./notification": {
      "types": "./notification/index.ts",
      "development": "./notification/index.ts",
      "default": "./dist/notification/index.js"
    },
    "./hooks": {
      "types": "./hooks/index.ts",
      "development": "./hooks/index.ts",
      "default": "./dist/hooks/index.js"
    },
    "./providers": {
      "types": "./providers/index.ts",
      "development": "./providers/index.ts",
      "default": "./dist/providers/index.js"
    },
    "./providers/theme": {
      "types": "./providers/theme/index.ts",
      "development": "./providers/theme/index.ts",
      "default": "./dist/providers/theme/index.js"
    },
    "./components/ui": {
      "types": "./components/ui/index.ts",
      "development": "./components/ui/index.ts",
      "default": "./dist/components/ui/index.js"
    },
    "./components/ui/*": {
      "types": "./components/ui/*.tsx",
      "development": "./components/ui/*.tsx",
      "default": "./dist/components/ui/*.js"
    },
    "./components/chat/*": {
      "types": "./components/chat/*.tsx",
      "development": "./components/chat/*.tsx",
      "default": "./dist/components/chat/*.js"
    },
    "./lib": {
      "types": "./lib/index.ts",
      "development": "./lib/index.ts",
      "default": "./dist/lib/index.js"
    },
    "./lib/chat-types": {
      "types": "./lib/chat-types.ts",
      "development": "./lib/chat-types.ts",
      "default": "./dist/lib/chat-types.js"
    },
    "./lib/emoji": {
      "types": "./lib/emoji.ts",
      "development": "./lib/emoji.ts",
      "default": "./dist/lib/emoji.js"
    },
    "./styles/theme.css": "./styles/theme.css"
  },
  "dependencies": {
    "@better-auth/passkey": "^1.4.9",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.19",
    "better-auth": "^1.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "frimousse": "0.3.0",
    "hono": "^4.11.0",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "react-i18next": "^15.4.1",
    "react-router": "^7.10.1",
    "shared": "workspace:*",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "~5.7.2"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "dev": "bun build ./index.ts ./auth/index.ts ./api/index.ts ./chat/index.ts ./notification/index.ts ./hooks/index.ts ./providers/index.ts ./providers/theme/index.ts ./components/ui/index.ts ./components/ui/*.tsx ./components/chat/*.tsx ./lib/index.ts ./lib/chat-types.ts ./lib/emoji.ts --outdir ./dist --splitting --packages external --watch",
    "build": "bun build ./index.ts ./auth/index.ts ./api/index.ts ./chat/index.ts ./notification/index.ts ./hooks/index.ts ./providers/index.ts ./providers/theme/index.ts ./components/ui/index.ts ./components/ui/*.tsx ./components/chat/*.tsx ./lib/index.ts ./lib/chat-types.ts ./lib/emoji.ts --outdir ./dist --splitting --packages external --production",
    "test": "bun test --preload ../../apps/frontend/happydom.ts --preload ../../apps/frontend/testing-library.ts",
    "test:coverage": "bun test --coverage --preload ../../apps/frontend/happydom.ts --preload ../../apps/frontend/testing-library.ts",
    "lint": "bun run biome lint ./index.ts ./interfaces ./config ./auth",
    "format": "bun run biome format --write ./index.ts ./interfaces ./config ./auth"
  }
}
