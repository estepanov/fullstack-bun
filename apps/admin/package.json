{
  "name": "admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "react-router dev --force",
    "dev:demo": "VITE_ADMIN_DEMO=true react-router dev --force",
    "build": "tsc -b && react-router build",
    "build:demo": "VITE_ADMIN_DEMO=true bun run build",
    "format": "bun run biome format --write ./src",
    "lint": "bun run biome lint ./src",
    "preview": "vite preview",
    "serve:demo": "bunx --bun serve ./build/client -l 4175",
    "test": "bun test --preload ../frontend/happydom.ts --preload ../frontend/testing-library.ts",
    "test:coverage": "bun test --coverage --preload ../frontend/happydom.ts --preload ../frontend/testing-library.ts",
    "clean": "rm -rf node_modules build .vite"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-router/node": "^7.10.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-form": "^1.27.7",
    "@testing-library/react": "^16.3.2",
    "@tanstack/react-query": "^5.90.19",
    "api": "workspace:*",
    "better-auth": "^1.4.9",
    "frontend-common": "workspace:*",
    "hono": "^4.11.0",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "isbot": "^5",
    "lucide-react": "^0.483.0",
    "msw": "2.3.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^15.4.1",
    "react-router": "^7.10.1",
    "shared": "workspace:*",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.18",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@react-router/dev": "^7.10.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/bun": "^1.3.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "happy-dom": "^20.3.4",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "msw": {
    "workerDirectory": "./public"
  }
}
